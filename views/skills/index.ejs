<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Developer Skills</h1>
    <ul>
        <% skills.forEach(function(skill){ %>
            <li>
                <a href="/skills/<%= skill.devSkill%>"><%= skill.devSkill %></a>  
                :
                <input type="checkbox" ><%=skill.done ? 'Done' : 'Not Done'%>
            </li>
        <%})  %> 
    </ul>
    <a href="/">Home</a>
   <a href="/skills">Developer Skills</a>
   <a href="/skills/show">Completed Skills</a> 
</body>
</html>